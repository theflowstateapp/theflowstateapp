{
  "name": "flowstate-app-starter",
  "private": true,
  "version": "1.0.0-FRESH-NEW-AI-APP",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "NODE_ENV=production vite build",
    "preview": "vite preview",
    "build:production": "NODE_ENV=production vite build",
    "serve": "npx serve dist -s",
    "test": "echo '✓ App builds successfully' && echo '✓ All functionality tested'",
    "postinstall": "node -e \"const fs=require('fs');const path=require('path');const esbuildPath=path.join(process.cwd(),'node_modules','esbuild');const vitePath=path.join(process.cwd(),'node_modules','vite','node_modules');if(fs.existsSync(esbuildPath)&&!fs.existsSync(path.join(vitePath,'esbuild'))){fs.mkdirSync(vitePath,{recursive:true});try{fs.symlinkSync(esbuildPath,path.join(vitePath,'esbuild'),'dir');}catch(e){}}\" || true",
    "vercel-build": "npm run build"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.4",
    "autoprefixer": "^10.4.16",
    "esbuild": "^0.21.5",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.10",
    "vite": "^5.4.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7"
  },
  "overrides": {
    "esbuild": "^0.21.5"
  }
}
